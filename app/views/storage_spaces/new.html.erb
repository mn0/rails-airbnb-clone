<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-6 col-md-offset-3" id="create_storage">
      <%= simple_form_for [@storage_space] do |f| %>
      <%= f.input :location %>
      <%= f.input :space %>
      <%= f.input :photo, as: :attachinary %>
      <%= f.submit "submit", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>



